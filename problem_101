#include <stdio.h>
#include <string.h>

int main() {
    char str[100];
    int i = 0, j = 0;

    printf("Enter a number/string: ");
    fgets(str, sizeof(str), stdin);
    while (str[i] == '0') {
        i++;
    }
    if (str[i] == '\n' || str[i] == '\0') {
        printf("0\n");
        return 0;
    }
    for (j = i; str[j] != '\0'; j++) {
        printf("%c", str[j]);
    }

    return 0;
}
